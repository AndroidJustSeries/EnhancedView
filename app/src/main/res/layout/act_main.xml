        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingBottom="10dp"
        android:text="EnhancedImageView Sample"
        android:textColor="#000000"
        android:textSize="16sp"
        app:BgRadius="5dp"
        app:BgStrokeColorNormal="#111111"
        app:BgStrokeColorSelected="#727af2"
        app:StrokeWidth="1dp"
        app:cfont="extrabold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:textColorNormal="#111111"
        app:textColorSelected="#2d2d2d" />

    <com.kds.just.enhancedview.view.EnhancedTextView
        android:id="@+id/sampleEnhancedScaleImageview"
